{
  "name": "vue-ant-demo",
  "version": "0.1.0",
  "homepage": "https://pomeloman.github.io/vue-ant-demo",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode dev",
    "build:dev": "vue-cli-service build --mode dev",
    "build:gh": "vue-cli-service build --mode gh",
    "build:test": "vue-cli-service build --mode test",
    "build": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "predeploy": "npm run build:gh",
    "deploy": "node deploy.js",
    "deploy:gh-pages": "npm run build:gh && deploy.bat",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "@antv/g2": "^3.5.11",
    "accounting": "^0.4.1",
    "ant-design-vue": "^1.4.11",
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "date-fns": "^2.8.1",
    "date-fns-tz": "^1.0.8",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "nprogress": "^0.2.0",
    "perfect-scrollbar": "^1.5.0",
    "qs": "^6.9.1",
    "sass-loader": "^8.0.0",
    "three": "^0.111.0",
    "three-trackballcontrols": "0.0.8",
    "tween": "^0.9.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.15.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.12.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "gh-pages": "^2.1.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "prettier": "1.18.2",
    "svg-sprite-loader": "^4.1.6",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
